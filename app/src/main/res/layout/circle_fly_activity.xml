<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/circle_fly_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.jamin.android.demo.widget.CornerLayout
        android:id="@+id/circle_vertical_fly_layout"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_centerHorizontal="true"
        android:background="#000fff">

    </com.jamin.android.demo.widget.CornerLayout>

    <com.jamin.android.demo.widget.CornerLayout2
        android:id="@+id/circle_vertical_fly_layout2"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_below="@+id/circle_vertical_fly_layout"
        android:layout_centerHorizontal="true"
        android:background="#000fff">

    </com.jamin.android.demo.widget.CornerLayout2>


</RelativeLayout>